const HeroBanner = ({ title, subtitle, buttonText }) => {
  return (
    <div className="bg-gradient-to-br from-blue-900 via-blue-800 to-indigo-900 text-white py-24 px-6 relative overflow-hidden">
      <div className="absolute inset-0 opacity-10">
        <div className="absolute top-1/2 left-1/4 transform -translate-x-1/2 -translate-y-1/2 text-9xl font-bold text-gray-300">
          1200
        </div>
        <div className="absolute top-1/2 right-1/4 transform translate-x-1/2 -translate-y-1/2 text-9xl font-bold text-gray-300">
          4400
        </div>
      </div>
      <div className="container mx-auto text-center relative z-10">
        <h2 className="text-5xl font-bold mb-4">{title}</h2>
        <p className="text-xl mb-8 text-blue-100">{subtitle}</p>
        <button className="bg-orange-500 hover:bg-orange-600 text-white font-bold py-3 px-8 rounded-lg transition-all duration-200 transform hover:scale-105 shadow-lg">
          {buttonText}
        </button>
      </div>
    </div>
  );
};

// Product Card Component
const ProductCard = ({ name, description, price, image }) => {
  return (
    <div className="bg-white rounded-lg shadow-md overflow-hidden hover:shadow-xl transition-shadow duration-300 flex flex-col h-full">
      <div className="bg-gray-200 h-64 flex items-center justify-center overflow-hidden">
        {image ? (
          <img src={image} alt={name} className="w-full h-full object-cover" />
        ) : (
          <span className="text-gray-400 text-xl">600 × 400</span>
        )}
      </div>
      <div className="p-6 flex-1 flex flex-col">
        <h3 className="text-xl font-bold mb-2 text-gray-800">{name}</h3>
        <p className="text-gray-600 text-sm mb-4 flex-1">{description}</p>
        <div className="flex justify-between items-center mt-auto">
          <span className="text-2xl font-bold text-green-600">${price}</span>
          <button className="bg-blue-600 hover:bg-blue-700 text-white font-semibold py-2 px-4 rounded transition-colors duration-200">
            Add to Cart
          </button>
        </div>
      </div>
    </div>
  );
};

